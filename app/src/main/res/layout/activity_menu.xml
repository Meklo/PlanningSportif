<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="fr.utt.if26.planningsportif.Activities.MenuActivity">


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_weight="0.65"
        android:orientation="horizontal"
        tools:context="fr.utt.if26.planningsportif.Activities.MenuActivity"
        android:weightSum="1">





    </LinearLayout>

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:layout_weight="0.04"
        android:orientation="horizontal"
        tools:context="fr.utt.if26.planningsportif.Activities.MenuActivity"
        android:weightSum="1">

        <Button
            android:id="@+id/nouveauProgrammeBtnMenu"
            android:layout_width="123dp"
            android:layout_height="match_parent"
            android:text="@string/nouveauProgrammeBtnMenu" />



        <ImageView
            android:id="@+id/imageView"
            android:layout_width="40dp"
            android:layout_height="70dp"
            app:srcCompat="@android:drawable/ic_menu_add" />


        <Button
            android:id="@+id/listProgrammeBtnMenu"
            android:layout_width="wrap_content"
            android:layout_height="72dp"
            android:text="@string/listProgrammeBtnMenu" />

        <ImageView
            android:id="@+id/imageView2"
            android:layout_width="match_parent"
            android:layout_height="73dp"
            app:srcCompat="@android:drawable/ic_menu_sort_by_size" />

    </LinearLayout>


</LinearLayout>
